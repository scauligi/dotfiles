[user]
	name   = sunjayc
	email  = sunjayc@cs.washington.edu
[color]
	diff = auto
	ui = auto
[log]
	date = local
[blame]
	date = local
[alias]
	graph = log --decorate --all --graph
	sb = status -sb
	lg = log --graph --branches --all --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
[push]
	default = simple
[core]
	excludesfile = /homes/iws/sunjayc/.gitignore
[merge]
	conflictstyle = diff3
